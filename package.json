    "test-rinkeby-unlock": "react-scripts test --preset @chainsafe/dappeteer src/tests/rinkeby/desktop/unlockTokenModal.test.ts",
    "coverage": "yarn test-rinkeby -- --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"

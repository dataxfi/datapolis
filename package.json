    "test-uiLP": "react-scripts test --preset @chainsafe/dappeteer src/tests/ui/uiLP.test.ts",
    "test-uiDtDesc": "react-scripts test --preset @chainsafe/dappeteer src/tests/ui/uiDtDesc.test.ts",
